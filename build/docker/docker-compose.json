{
  "shares": {
    "image": "tianon/true",
    "volumes": [
      "./media:/data",
      "./configs:/configs",
      "./temp:/temp"
    ]
  },
  "sabnzbd": {
    "image": "timhaak/sabnzbd",
    "volumes_from": [
      "shares"
    ],
    "volumes": [
      "./configs/sabnzbd:/config",
      "./temp/download:/incomplete",
      "./temp/complete:/complete",
      "./temp/complete/TV:/tv",
      "./temp/complete/Movies:/movies"
    ],
    "ports": [
      "8080:8080",
      "9090:9090"
    ],
    "extra_hosts": [
      "0.0.0.0:localhost"
    ],
    "restart": "always"
  },
  "sickgear": {
    "image": "ressu/sickgear",
    "volumes_from": [
      "shares"
    ],
    "volumes": [
      "./configs/sickgear:/data",
      "./temp/complete/TV:/incoming",
      "./media/TV:/media/TV"
    ],
    "links": [
      "sabnzbd"
    ],
    "ports": [
      "8081:8081"
    ],
    "extra_hosts": [
      "0.0.0.0:localhost"
    ],
    "restart": "always"
  }
}
